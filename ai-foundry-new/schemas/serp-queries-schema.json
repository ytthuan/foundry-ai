{
  "name": "serp_queries",
  "strict": true,
  "schema": {
    "type": "object",
    "properties": {
      "queries": {
        "type": "array",
        "description": "Array of SERP query strings (keywords only)",
        "items": {
          "type": "string"
        },
        "maxItems": 10
      },
      "research_goals": {
        "type": "array",
        "description": "Array of research goals corresponding to each query (same order)",
        "items": {
          "type": "string"
        },
        "maxItems": 10
      }
    },
    "additionalProperties": false,
    "required": ["queries", "research_goals"]
  }
}
